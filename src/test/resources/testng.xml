<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Playwright Suite">

    <listeners>
        <listener class-name="base.ArtifactListener"/>
    </listeners>

    <test name="Smoke Tests">
        <groups>
            <run>
                <include name="smoke"/>
            </run>
        </groups>

        <classes>
            <class name="tests.PlaywrightDocsTest"/>
            <class name="tests.PlaywrightDocsSearchTest"/>
        </classes>
    </test>

</suite>
